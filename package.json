{
  "name": "node-backend-benchmark",
  "version": "0.0.1",
  "description": "autogenerated graphql backend benchmark",
  "main": "index.js",
  "author": "cudr",
  "license": "MIT",
  "scripts": {
    "start": "node benchmark.js && node benchmark.js compare -t",
    "bench": "node benchmark.js",
    "compare": "node benchmark.js compare -t",
    "update-readme": "sed -n '1,21p' README.md > tmp.md && mv tmp.md README.md && node benchmark.js compare -tc >> README.md",
    "format": "prettier --write"
  },
  "dependencies": {
    "autocannon-compare": "^0.4.0",
    "chalk": "^4.1.0",
    "cli-table": "^0.3.1",
    "commander": "^6.2.0",
    "graphql-codegen": "^0.4.0",
    "inquirer": "^7.3.3",
    "ora": "^5.1.0"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,babelrc,yml}": [
      "yarn run format",
      "git add"
    ]
  }
}
